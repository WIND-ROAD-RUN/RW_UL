/* 标题样式 */
QLabel.title {
    font-size: 16px;
    font-weight: 600;
    color: #333333;
    border-bottom: 1px solid #DDDDDD; /* 下划线装饰 */
    padding-bottom: 4px;
}

/* 副标题/次要文本 */
QLabel.subtitle {
    color: #666666;
    font-size: 13px;
    font-style: italic;
}

/* 强调文本 */
QLabel.emphasis {
    color: #333333;
    font-weight: 500;
    background-color: #F5F5F5;
    border-radius: 3px;
    padding: 4px 8px;
}

/* 状态标签（成功/警告/错误） */
QLabel[state=&quot;success&quot;] {
    color: #228B22;          /* 森林绿 */
    border-left: 3px solid #228B22;
    padding-left: 8px;
}

QLabel[state=&quot;warning&quot;] {
    color: #FF8C00;          /* 暗橙 */
    border-left: 3px solid #FF8C00;
    padding-left: 8px;
}

QLabel[state=&quot;error&quot;] {
    color: #B22222;          /* 砖红 */
    border-left: 3px solid #B22222;
    padding-left: 8px;
}

/* 数据展示标签 */
QLabel.data {
    color: #2F4F4F;          /* 深石板灰 */
