    border: 1px solid #E0E0E0;
    background-color: #FFFFFF;
    padding: 6px 12px;
    border-radius: 4px;
}

/* 禁用状态 */
QLabel:disabled {
    color: #AAAAAA;
    background-color: transparent;
}

/* 链接样式（伪链接效果） */
QLabel.link {
    color: #4682B4;          /* 钢蓝 */
    text-decoration: underline;
    border-bottom: 1px dotted #4682B4;
}

/* 图标标签 */
QLabel.icon {
    qproperty-alignment: AlignCenter;
    border: 1px solid #E0E0E0;
    border-radius: 4px;
    background-color: #FFFFFF;
    padding: 12px;
}</string>
