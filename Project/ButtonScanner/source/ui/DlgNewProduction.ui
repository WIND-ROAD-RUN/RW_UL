    font-size: 14px;
    font-family: &quot;Consolas&quot;, monospace;
    padding: 4px;
    selection-background-color: #666666;
    selection-color: #FFFFFF;
}

/* 光标样式 */
QPlainTextEdit::cursor {
    width: 1.5px;
    background: #444444;
}

/* 行号栏 */
QPlainTextEdit .linenumber {
    background: #F8F8F8;
    border-right: 1px solid #DDDDDD;
    color: #888888;
    padding: 0 8px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 13px;
    qproperty-alignment: AlignRight;
}

/* 当前行高亮 */
QPlainTextEdit::current-line {
    background: #F5F5F5;
    border: none;
}

/* 断点标记 */
QPlainTextEdit[breakpoint=&quot;true&quot;]::block-mark {
    background: #FF6666;
    border-radius: 4px;
    width: 12px;
    margin: 2px;
}

/* 代码折叠区域 */
QPlainTextEdit::fold-area {
    background: #F0F0F0;
    border: 1px solid #DDDDDD;
}

/* 滚动条系统 */
QScrollBar:vertical {
    width: 10px;
    background: #FFFFFF;
}

QScrollBar::handle:vertical {
    background: #CCCCCC;
    border-radius: 4px;
    min-height: 20px;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: transparent;
}

/* 禁用状态 */
QPlainTextEdit:disabled {
    background: #F5F5F5;
    color: #AAAAAA;
    border-color: #DDDDDD;
}

/* 只读状态 */
QPlainTextEdit[readOnly=&quot;true&quot;] {
    background: #FFFFFF;
    border-color: #CCCCCC;
}

/* 错误波浪线 */
QPlainTextEdit::error-wave {
    color: #FF6666;
    wave-height: 2px;
    wave-length: 4px;
}

/* 搜索高亮 */
