    background: #FFD700;
    border-radius: 2px;
}

/* 紧凑型变体 */
QPlainTextEdit.compact {
    font-size: 13px;
    padding: 2px;
}

QPlainTextEdit.compact .linenumber {
    font-size: 12px;
    padding: 0 6px;
}</string>
