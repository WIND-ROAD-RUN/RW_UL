          <item>
           <widget class="QProgressBar" name="progressBar_tab5">
            <property name="styleSheet">
             <string notr="true">/* 基础进度条 */
QProgressBar {
    border: 1px solid #AAAAAA;
    border-radius: 3px;
    background: #FFFFFF;
    color: #444444;
    font-size: 14px;
    text-align: center;
    height: 24px;
}

/* 进度填充 */
QProgressBar::chunk {
    background: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #888888,
        stop:1 #666666
    );
    border-radius: 2px;
    margin: 2px;
}

/* 状态变体 */
QProgressBar[state=&quot;success&quot;]::chunk {
    background: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #88AA88,
        stop:1 #669966
    );
}

QProgressBar[state=&quot;error&quot;]::chunk {
    background: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #CC6666,
        stop:1 #AA4444
    );
}

QProgressBar[state=&quot;warning&quot;]::chunk {
    background: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #FFD700,
        stop:1 #D4B000
    );
}

/* 禁用状态 */
QProgressBar:disabled {
    border-color: #DDDDDD;
    background: #F5F5F5;
    color: #AAAAAA;
}

QProgressBar:disabled::chunk {
    background: #DDDDDD;
}

/* 紧凑模式 */
QProgressBar.compact {
    height: 16px;
    font-size: 12px;
    border-radius: 2px;
}

QProgressBar.compact::chunk {
    margin: 1px;
}

/* 垂直模式 */
QProgressBar.vertical {
    width: 24px;
    height: 200px;
}

QProgressBar.vertical::chunk {
    width: 100%;
    margin: 2px 0;
}

/* 不确定模式 */
QProgressBar:indeterminate::chunk {
    background: repeating-linear-gradient(
        45deg,
        #666666,
        #666666 10px,
        #888888 10px,
        #888888 20px
    );
    animation: progress-stripes 1s linear infinite;
}

@keyframes progress-stripes {
    from { background-position: 0 0; }
    to { background-position: 40px 0; }
}

/* 环形进度条变体 */
QProgressBar.circular {
    border-radius: 50%;
    border: 2px solid #AAAAAA;
    width: 64px;
    height: 64px;
    qproperty-format: &quot;&quot;;
}

QProgressBar.circular::chunk {
    border-radius: 50%;
    background: conic-gradient(
        from 90deg,
        #666666 0%,
        #666666 var(--progress),
        transparent var(--progress),
        transparent 100%
    );
}</string>
